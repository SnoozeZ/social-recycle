<% provide(:title, @user.username) %>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-4 col-md-offset-4" style = text-align:center>
      <h1><%= @user.username %></h1>
      <p><span>Reg at: </span><%= @user.reg_time %>
    </div>
  </div>
  <div class="container" style="margin-top:20px;">
    <ul class="nav nav-tabs" id="usertabs">
      <li class="active" ><a data-toggle="tab" href="#dropped">Dropped Items</a></li>
      <li><a data-toggle="tab" href="#dibs">Dibs Items</a></li>
    </ul>
    <div id="tabcontent" class="tab-content">
      <div id="dropped" class="tab-pane fade in active">
        <ul style="margin-top: 10px;">
          <% @items.each do |s| %>
              <li><%= link_to s.title, s %></li>
          <%end%>
        </ul>

        <%= will_paginate @items, class:"center" %>
      </div>
      <div id="dibs" class="tab-pane fade">
        <ul style="margin-top: 10px;">
          <% @dibs_items.each do |item| %>
              <li><%= link_to item.title, item %></li>
          <% end %>
        </ul>
        <%= will_paginate @dibs_items, class: "center"%>
      </div>

    </div>
    </div>
  </div>
</div>

